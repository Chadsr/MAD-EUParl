@prefix dbo: <http://dbpedia.org/ontology/> .
@prefix dbp: <http://dbpedia.org/property/> .
@prefix dbr: <http://dbpedia.org/resource/> .
@prefix epv: <http://localhost:7200/parlialytics#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://localhost:7200/parlialytics#> rdf:type owl:Ontology .

#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://dbpedia.org/ontology/PoliticalParty
dbo:PoliticalParty rdf:type owl:ObjectProperty .


###  http://dbpedia.org/ontology/birthPlace
dbo:birthPlace rdf:type owl:ObjectProperty ,
                        owl:FunctionalProperty ;
               rdfs:domain dbo:Person ;
               rdfs:range dbo:Place .


###  http://dbpedia.org/ontology/gender
dbo:gender rdf:type owl:ObjectProperty ,
                    owl:FunctionalProperty .


###  http://dbpedia.org/ontology/party
dbo:party rdf:type owl:ObjectProperty ;
          rdfs:range dbo:PoliticalParty .


###  http://dbpedia.org/ontology/politicalPartyInLegislature
dbo:politicalPartyInLegislature rdf:type owl:ObjectProperty ;
                                rdfs:domain dbo:Legislature ;
                                rdfs:range dbo:PoliticalParty .


###  http://dbpedia.org/property/houses
dbp:houses rdf:type owl:ObjectProperty ;
           owl:inverseOf epv:processesFor ;
           rdfs:range epv:VotingAssembly .


###  http://dbpedia.org/property/office
dbp:office rdf:type owl:ObjectProperty .


###  http://localhost:7200/parlialytics#abstains
epv:abstains rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf epv:reactsToVote ;
             rdfs:domain epv:Voter ;
             rdfs:range epv:Dossier .


###  http://localhost:7200/parlialytics#geoArea
epv:geoArea rdf:type owl:ObjectProperty ;
            rdfs:domain epv:Dossier ;
            rdfs:range dbo:Place .


###  http://localhost:7200/parlialytics#hasActivity
epv:hasActivity rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf owl:topObjectProperty ;
                rdfs:domain epv:Dossier ;
                rdfs:range epv:Activity .


###  http://localhost:7200/parlialytics#hasBody
epv:hasBody rdf:type owl:ObjectProperty ;
            rdfs:domain epv:Activity ;
            rdfs:range dbo:Organisation .


###  http://localhost:7200/parlialytics#hasMeeting
epv:hasMeeting rdf:type owl:ObjectProperty ;
               rdfs:domain epv:Committee ;
               rdfs:range epv:Meeting .


###  http://localhost:7200/parlialytics#hasMember
epv:hasMember rdf:type owl:ObjectProperty ;
              rdfs:domain epv:Group ;
              rdfs:range dbo:Person .


###  http://localhost:7200/parlialytics#hasMembership
epv:hasMembership rdf:type owl:ObjectProperty ;
                  owl:inverseOf epv:membershipOf ;
                  rdfs:domain epv:MEP ;
                  rdfs:range epv:Membership .


###  http://localhost:7200/parlialytics#hasRapporteur
epv:hasRapporteur rdf:type owl:ObjectProperty ;
                  rdfs:domain epv:Committee ;
                  rdfs:range epv:MEP .


###  http://localhost:7200/parlialytics#hasText
epv:hasText rdf:type owl:ObjectProperty ,
                     owl:FunctionalProperty ;
            rdfs:domain epv:Dossier ;
            rdfs:range [ rdf:type owl:Restriction ;
                         owl:onProperty epv:dossierTItle ;
                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                         owl:onDataRange xsd:string
                       ] .


###  http://localhost:7200/parlialytics#hasVoter
epv:hasVoter rdf:type owl:ObjectProperty ;
             owl:inverseOf epv:votesIn ;
             rdfs:domain epv:VotingAssembly ;
             rdfs:range epv:Voter .


###  http://localhost:7200/parlialytics#involves
epv:involves rdf:type owl:ObjectProperty ;
             owl:inverseOf epv:isInvolvedIn .


###  http://localhost:7200/parlialytics#isInvolvedIn
epv:isInvolvedIn rdf:type owl:ObjectProperty ;
                 rdfs:domain epv:Committee ;
                 rdfs:range epv:Dossier .


###  http://localhost:7200/parlialytics#isResponsibleIn
epv:isResponsibleIn rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf epv:isInvolvedIn .


###  http://localhost:7200/parlialytics#isWithin
epv:isWithin rdf:type owl:ObjectProperty ;
             rdfs:domain epv:Membership ;
             rdfs:range epv:Group .


###  http://localhost:7200/parlialytics#membershipOf
epv:membershipOf rdf:type owl:ObjectProperty .


###  http://localhost:7200/parlialytics#processed
epv:processed rdf:type owl:ObjectProperty ;
              owl:inverseOf epv:processedBy ;
              rdfs:domain epv:VotingAssembly ;
              rdfs:range epv:Dossier ;
              rdfs:label "processed"@en .


###  http://localhost:7200/parlialytics#processedBy
epv:processedBy rdf:type owl:ObjectProperty ;
                rdfs:domain epv:Dossier ;
                rdfs:range epv:VotingAssembly ;
                rdfs:label "processedBy"@en .


###  http://localhost:7200/parlialytics#processesFor
epv:processesFor rdf:type owl:ObjectProperty ;
                 rdfs:domain epv:VotingAssembly .


###  http://localhost:7200/parlialytics#reactsToVote
epv:reactsToVote rdf:type owl:ObjectProperty ;
                 rdfs:domain epv:Voter ;
                 rdfs:range epv:Dossier .


###  http://localhost:7200/parlialytics#representedBy
epv:representedBy rdf:type owl:ObjectProperty ;
                  owl:inverseOf epv:represents ;
                  rdfs:range epv:Voter ;
                  rdfs:label "representedBy"@en .


###  http://localhost:7200/parlialytics#represents
epv:represents rdf:type owl:ObjectProperty ;
               rdfs:domain epv:Voter ;
               rdfs:label "represents"@en .


###  http://localhost:7200/parlialytics#representsCountry
epv:representsCountry rdf:type owl:ObjectProperty ,
                               owl:FunctionalProperty ;
                      rdfs:domain epv:Membership ;
                      rdfs:range dbo:Country .


###  http://localhost:7200/parlialytics#votesAgainst
epv:votesAgainst rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf epv:votesOn ;
                 rdfs:domain epv:Voter ;
                 rdfs:range epv:Dossier .


###  http://localhost:7200/parlialytics#votesFor
epv:votesFor rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf epv:votesOn ;
             rdfs:domain epv:Voter ;
             rdfs:range epv:Dossier .


###  http://localhost:7200/parlialytics#votesIn
epv:votesIn rdf:type owl:ObjectProperty ;
            rdfs:domain epv:Voter ;
            rdfs:range epv:VotingAssembly ;
            rdfs:label "votesIn"@en .


###  http://localhost:7200/parlialytics#votesOn
epv:votesOn rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf epv:reactsToVote ;
            rdfs:domain epv:Voter ;
            rdfs:range epv:Dossier .


#################################################################
#    Data properties
#################################################################

###  http://dbpedia.org/ontology/birthDate
dbo:birthDate rdf:type owl:DatatypeProperty ,
                       owl:FunctionalProperty ;
              rdfs:domain dbo:Person ;
              rdfs:range xsd:date .


###  http://dbpedia.org/ontology/deathDate
dbo:deathDate rdf:type owl:DatatypeProperty ,
                       owl:FunctionalProperty ;
              rdfs:domain dbo:Person ;
              rdfs:range xsd:date .


###  http://localhost:7200/parlialytics#dossierTItle
epv:dossierTItle rdf:type owl:DatatypeProperty ,
                          owl:FunctionalProperty ;
                 rdfs:domain epv:Dossier ;
                 rdfs:range xsd:string .


###  http://localhost:7200/parlialytics#endDate
epv:endDate rdf:type owl:DatatypeProperty ,
                     owl:FunctionalProperty ;
            rdfs:domain epv:Membership ;
            rdfs:range xsd:date .


###  http://localhost:7200/parlialytics#startDate
epv:startDate rdf:type owl:DatatypeProperty ,
                       owl:FunctionalProperty ;
              rdfs:domain epv:Membership ;
              rdfs:range xsd:date .


###  http://xmlns.com/foaf/0.1/age
foaf:age rdf:type owl:DatatypeProperty ,
                  owl:FunctionalProperty ;
         rdfs:domain foaf:Agent .


###  http://xmlns.com/foaf/0.1/gender
foaf:gender rdf:type owl:DatatypeProperty ,
                     owl:FunctionalProperty ;
            rdfs:domain foaf:Agent .


###  http://xmlns.com/foaf/0.1/name
foaf:name rdf:type owl:DatatypeProperty .


#################################################################
#    Classes
#################################################################

###  epv:Activity
epv:Activity rdf:type owl:Class .


###  http://dbpedia.org/ontology/Country
dbo:Country rdf:type owl:Class ;
            rdfs:subClassOf dbo:Place .


###  http://dbpedia.org/ontology/Legislature
dbo:Legislature rdf:type owl:Class ;
                rdfs:subClassOf dbo:Organisation .


###  http://dbpedia.org/ontology/Organisation
dbo:Organisation rdf:type owl:Class .


###  http://dbpedia.org/ontology/Person
dbo:Person rdf:type owl:Class ;
           rdfs:subClassOf epv:Agent .


###  http://dbpedia.org/ontology/Place
dbo:Place rdf:type owl:Class .


###  http://dbpedia.org/ontology/PoliticalParty
dbo:PoliticalParty rdf:type owl:Class .


###  http://localhost:7200/parlialytics#ActiveMEP
epv:ActiveMEP rdf:type owl:Class ;
              owl:equivalentClass [ owl:intersectionOf ( epv:MEP
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty epv:hasMembership ;
                                                           owl:someValuesFrom epv:ActiveMembership
                                                         ]
                                                       ) ;
                                    rdf:type owl:Class
                                  ] ;
              rdfs:subClassOf epv:MEP ;
              owl:disjointWith epv:InactiveMEP .


###  http://localhost:7200/parlialytics#ActiveMembership
epv:ActiveMembership rdf:type owl:Class ;
                     rdfs:subClassOf epv:Membership ;
                     owl:disjointWith epv:InactiveMembership .


###  http://localhost:7200/parlialytics#Agent
epv:Agent rdf:type owl:Class ;
          owl:equivalentClass foaf:Agent ;
          rdfs:label "Agent"@en .


###  http://localhost:7200/parlialytics#Chair
epv:Chair rdf:type owl:Class ;
          rdfs:subClassOf epv:Membership .


###  http://localhost:7200/parlialytics#ChairOfTheBureau
epv:ChairOfTheBureau rdf:type owl:Class ;
                     rdfs:subClassOf epv:Membership .


###  http://localhost:7200/parlialytics#CoChair
epv:CoChair rdf:type owl:Class ;
            rdfs:subClassOf epv:Membership .


###  http://localhost:7200/parlialytics#CoTreasurer
epv:CoTreasurer rdf:type owl:Class ;
                rdfs:subClassOf epv:Membership .


###  http://localhost:7200/parlialytics#Committee
epv:Committee rdf:type owl:Class ;
              rdfs:subClassOf epv:Group .


###  http://localhost:7200/parlialytics#DeputyChair
epv:DeputyChair rdf:type owl:Class ;
                rdfs:subClassOf epv:Membership .


###  http://localhost:7200/parlialytics#DeputyTreasurer
epv:DeputyTreasurer rdf:type owl:Class ;
                    rdfs:subClassOf epv:Membership .


###  http://localhost:7200/parlialytics#Dossier
epv:Dossier rdf:type owl:Class ;
            owl:equivalentClass [ rdf:type owl:Restriction ;
                                  owl:onProperty epv:processedBy ;
                                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                  owl:onClass epv:VotingAssembly
                                ] .


###  http://localhost:7200/parlialytics#FemaleMEP
epv:FemaleMEP rdf:type owl:Class ;
              owl:equivalentClass [ owl:intersectionOf ( epv:MEP
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty foaf:gender ;
                                                           owl:hasValue "female"^^xsd:string
                                                         ]
                                                       ) ;
                                    rdf:type owl:Class
                                  ] ;
              rdfs:subClassOf epv:MEP .


###  http://localhost:7200/parlialytics#Group
epv:Group rdf:type owl:Class ;
          rdfs:subClassOf epv:Agent .


###  http://localhost:7200/parlialytics#InactiveMEP
epv:InactiveMEP rdf:type owl:Class ;
                owl:equivalentClass [ owl:intersectionOf ( epv:MEP
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty epv:hasMembership ;
                                                             owl:qualifiedCardinality "0"^^xsd:nonNegativeInteger ;
                                                             owl:onClass epv:ActiveMembership
                                                           ]
                                                         ) ;
                                      rdf:type owl:Class
                                    ] ;
                rdfs:subClassOf epv:MEP .


###  http://localhost:7200/parlialytics#InactiveMembership
epv:InactiveMembership rdf:type owl:Class ;
                       owl:equivalentClass [ owl:intersectionOf ( epv:Membership
                                                                  [ rdf:type owl:Restriction ;
                                                                    owl:onProperty epv:endDate ;
                                                                    owl:someValuesFrom xsd:date
                                                                  ]
                                                                ) ;
                                             rdf:type owl:Class
                                           ] ;
                       rdfs:subClassOf epv:Membership .


###  http://localhost:7200/parlialytics#MEP
epv:MEP rdf:type owl:Class ;
        rdfs:subClassOf dbo:Person .


###  http://localhost:7200/parlialytics#MaleMEP
epv:MaleMEP rdf:type owl:Class ;
            owl:equivalentClass [ owl:intersectionOf ( epv:MEP
                                                       [ rdf:type owl:Restriction ;
                                                         owl:onProperty foaf:gender ;
                                                         owl:hasValue "male"^^xsd:string
                                                       ]
                                                     ) ;
                                  rdf:type owl:Class
                                ] ;
            rdfs:subClassOf epv:MEP .


###  http://localhost:7200/parlialytics#Meeting
epv:Meeting rdf:type owl:Class .


###  http://localhost:7200/parlialytics#MemberOfTheBureau
epv:MemberOfTheBureau rdf:type owl:Class ;
                      rdfs:subClassOf epv:Membership .


###  http://localhost:7200/parlialytics#Membership
epv:Membership rdf:type owl:Class ;
               rdfs:comment "A type of membership within a epv:Group"@en .


###  http://localhost:7200/parlialytics#Observer
epv:Observer rdf:type owl:Class ;
             rdfs:subClassOf epv:Membership .


###  http://localhost:7200/parlialytics#Party
epv:Party rdf:type owl:Class ;
          rdfs:subClassOf epv:Group ;
          rdfs:comment "A collection of relatively likeminded Voters" ;
          rdfs:label "Party"@en .


###  http://localhost:7200/parlialytics#President
epv:President rdf:type owl:Class ;
              rdfs:subClassOf epv:Membership .


###  http://localhost:7200/parlialytics#SubCommittee
epv:SubCommittee rdf:type owl:Class ;
                 rdfs:subClassOf epv:Committee .


###  http://localhost:7200/parlialytics#Substitute
epv:Substitute rdf:type owl:Class ;
               rdfs:subClassOf epv:Membership .


###  http://localhost:7200/parlialytics#TemporaryCommittee
epv:TemporaryCommittee rdf:type owl:Class ;
                       rdfs:subClassOf epv:Committee .


###  http://localhost:7200/parlialytics#Treasurer
epv:Treasurer rdf:type owl:Class ;
              rdfs:subClassOf epv:Membership .


###  http://localhost:7200/parlialytics#ViceChair
epv:ViceChair rdf:type owl:Class ;
              rdfs:subClassOf epv:Membership .


###  http://localhost:7200/parlialytics#ViceChairOfTheBureau
epv:ViceChairOfTheBureau rdf:type owl:Class ;
                         rdfs:subClassOf epv:Membership .


###  http://localhost:7200/parlialytics#Voter
epv:Voter rdf:type owl:Class ;
          owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                       owl:onProperty epv:reactsToVote ;
                                                       owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                       owl:onClass epv:Dossier
                                                     ]
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty epv:votesIn ;
                                                       owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                       owl:onClass epv:VotingAssembly
                                                     ]
                                                   ) ;
                                rdf:type owl:Class
                              ] ;
          rdfs:subClassOf epv:Agent .


###  http://localhost:7200/parlialytics#VotingAssembly
epv:VotingAssembly rdf:type owl:Class ;
                   rdfs:subClassOf epv:Agent ;
                   rdfs:label "VotingAssembly"@en .


###  http://xmlns.com/foaf/0.1/Agent
foaf:Agent rdf:type owl:Class .
